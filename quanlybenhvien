import java.util.Scanner;
class BENHNHAN
{
private String hoten;
private int ngay;
private int thang;
private int nam;
private String diachi;
private String mabaohiem;
public BENHNHAN()
{

}
public BENHNHAN(String hoten, int ngay, int thang , int nam, String diachi, String mabaohiem)
{
    this.hoten = hoten;
    this.ngay=ngay;
    this.thang = thang;
    this.nam= nam;
    this.diachi=diachi;
    this.mabaohiem=mabaohiem;
}
public void Nhap()
{
    System.out.println("Ho ten benh nhan: ");
    Scanner sc = new Scanner (System.in);
    hoten = sc.nextLine();
    System.out.println("Nhap ngay thang nam sinh: ");
    ngay = sc.nextInt();
    thang = sc. nextInt();
    nam = sc.nextInt();
    System.out.println("Nhap dia chi: ");
    diachi = sc.nextLine();
    System.out.println("Nhap ma bao hiem neu co: ");
    mabaohiem = sc.nextLine();
}
public void Xuat()
{
    System.out.println("Thong tin nguoi benh: ");
    System.out.println("Ho ten: " + hoten);
    System.out.println("Ngay thang nam sinh: " +ngay+ "/" +thang+ "/" +nam);
    System.out.println("Dia chi: "+diachi);
    System.out.println("Ma bao hiem: "+mabaohiem);
}
}
class Nhanvien 
{
    private int age;
    private String hoten;
    private String diachi;
    private int manvv;
    public Nhanvien()
    {
        age = ;
        hoten = "";
        diachi = "";
        manv = ;
    }
    public Nhanvien(int age, string hoten, string diachi, int manv)
    {
        this.age = age;
        this.hoten = hoten;
        this.diachi = diachi;
        this.manv = manv;
    }
}
class bacsi extends Nhanvien()
{
    private int mabacsi;
    public bacsi()
    {

    }
    public bacsi(int age, string hoten, string diachi, int manv, int mabacsi)
    {
        this.age = age;
        this.hoten = hoten;
        this.diachi = diachi;
        this.manv = manv;
        this.mabacsi = mabacsi;
    }
    public void Nhap()
    {
        super.Nhap();
        Scanner sc = new Scanner(System.in);
        System.out.println("Nhap ma bac si:  ");
        mabacsi = sc.nextInt();
    }
}
class yta extends Nhanvien()
{
    private int mayta;
    public yta()
    {

    }
    public yta(int age, string hoten, string diachi, int manv, int mayta)
    {
        this.age = age;
        this.hoten = hoten;
        this.diachi = diachi;
        this.manv = manv;
        this.mayta = mayta;
    }
     public void Nhap()
    {
        super.Nhap();
        Scanner sc = new Scanner(System.in);
        System.out.println("Nhap ma y ta:  ");
        mayta = sc.nextInt();
    }

}
class letan extends Nhanvien()
{
    private int maletan;
    public letan()
    {

    }
    public letan(int age, string hoten, string diachi, int manv, int mabacsi)
    {
        this.age = age;
        this.hoten = hoten;
        this.diachi = diachi;
        this.manv = manv;
        this.maletan = maletan;
    }
 public void Nhap()
    {
        super.Nhap();
        Scanner sc = new Scanner(System.in);
        System.out.println("Nhap ma le tan:  ");
        maletan = sc.nextInt();
    }
}
